{
  "name": "govlens",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "postinstall": "prisma generate",
    "start": "next start",
    "lint": "eslint",
    "setup": "npm install && cp -n .env.example .env.local || true && npx prisma generate && npx prisma db push && npx tsx prisma/seed.ts",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:seed": "npx tsx prisma/seed.ts",
    "db:reset": "npx prisma db push --force-reset && npx tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "ingest": "curl -X POST http://localhost:3000/api/ingest",
    "bulk-ingest": "npx tsx --tsconfig tsconfig.json scripts/bulk-ingest.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.74.0",
    "@prisma/client": "^6.19.2",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.564.0",
    "next": "16.1.6",
    "openai": "^6.22.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "rss-parser": "^3.13.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "prisma": "^6.19.2",
    "tailwindcss": "^4",
    "tsx": "^4.21.0",
    "typescript": "^5"
  }
}
